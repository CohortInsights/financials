{
  "chart_type": "pie",

  "parameters": {
    "min_fraction": {
      "source": "plots.defaults.min_fraction",
      "overridable": true
    },
    "max_chart_count": {
      "source": "plots.defaults.max_chart_count",
      "overridable": false,
      "value": 4
    },
    "max_warning_chart_count": {
      "source": "plots.defaults.max_warning_chart_count",
      "overridable": false,
      "value": 4
    }
  },

  "data_patterns": {
    "minor_levels_present": true,
    "multiple_periods_present": true,
    "mixed_sign_present": true,
    "greater_than_max_charts": true
  },

  "pattern_policies": {
    "minor_levels_present": "ignore_minor_rows_and_warn",
    "multiple_periods_present": "chart_per_period",
    "mixed_sign_present": "ignore_minor_fraction_and_warn",
    "greater_than_max_charts": "limit_to_max_and_warn"
  },

  "warning_catalog": {
    "minor_levels_present": "$amount ($rows rows) of minor levels were ignored",
    "mixed_sign_present": "$amount ($rows rows) of $sign values were ignored",
    "greater_than_max_charts": "$rows charts beyond the first $max were ignored"
  },

  "interpretation": {
    "uses_major_level": true,
    "uses_minor_level": false,
    "abs_for_negative_only": true,
    "slice_basis": "major_level_rows",
    "multi_chart_dimension": "period"
  },

  "title": {
  "template": "{asn} (Level {level}) {duration_label}"
  },

  "labeling": {
    "strategy": "shortest_unique_trailing_fragment"
  },

  "layout": {
    "multi_pie_behavior": {
      "enabled": true,
      "grid_layout": true,
      "warn_if_exceeds_max": false
    }
  },

  "warnings": {
  "mixed_sign_present": "* {amount} ({rows} rows) of {sign} values were ignored",
  "minor_levels_present": "* {rows} rows from deeper assignment levels were ignored",
  "max_charts_truncated": "* Only the first {kept} of {total} charts were shown"
  },

  "other_slice": {
    "enabled": true,
    "combine_below_fraction": {
      "source": "parameters.min_fraction"
    },
    "label": "Other"
  }
}
