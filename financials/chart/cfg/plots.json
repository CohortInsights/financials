{
  "version": "1.0",

  "defaults": {
    "min_fraction": 0.025,
    "max_chart_count": 4,
    "rendering": {
      "figure_inches": 6,
      "dpi": 150
    }
  },

  "labeling": {
    "strategies": {
      "shortest_unique_trailing_fragment": {
        "description": "Use the shortest suffix of an assignment path that uniquely identifies each label within its peer set"
      }
    },
    "default_strategy": "shortest_unique_trailing_fragment"
  },

  "sign_handling": {
    "negative_only_mode": "absolute",
    "mixed_sign_supported_charts": [
      "bar"
    ]
  },

  "time_conventions": {
    "year_key": "sort_year",
    "period_key": "sort_period",
    "period_labels": {
      "annual": "Y",
      "quarterly": "Q",
      "monthly": "M"
    }
  },

  "ordering": {
    "assignment_order": "table_order_by_abs_amount",
    "stack_order": "table_order_bottom_up",
    "area_order": "table_order_bottom_up"
  },

  "layout": {
    "multi_chart_grid": {
      "max_columns": 2
    }
  },

  "rendering": {
    "backend": "matplotlib",
    "output_format": "png",
    "dpi": 150,
    "tight_layout": true
  },

  "palettes": {
    "4": [
      "#1f77b4",
      "#ff7f0e",
      "#2ca02c",
      "#d62728"
    ],
    "8": [
      "#1f77b4",
      "#ff7f0e",
      "#2ca02c",
      "#d62728",
      "#9467bd",
      "#8c564b",
      "#e377c2",
      "#7f7f7f"
    ],
    "16": [
      "#1f77b4",
      "#ff7f0e",
      "#2ca02c",
      "#d62728",
      "#9467bd",
      "#8c564b",
      "#e377c2",
      "#7f7f7f",
      "#bcbd22",
      "#17becf",
      "#aec7e8",
      "#ffbb78",
      "#98df8a",
      "#ff9896",
      "#c5b0d5",
      "#c49c94"
    ]
  },

  "palette_defaults": {
    "max_palette_size": 16,
    "reuse_strategy": "modulo",
    "reuse_warning": true,
    "reserved_colors": {
      "Other": "#cccccc",
      "Unassigned": "#999999"
    }
  }
}
